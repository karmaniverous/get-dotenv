{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".tsbuild",
    "paths": {
      "@/*": ["*"],
      "@karmaniverous/get-dotenv/cliHost": ["src/cliHost/index.ts"],
      "@karmaniverous/get-dotenv/plugins": ["src/plugins/index.ts"]
    },
    "tsBuildInfoFile": ".tsbuild/tsconfig.tsbuildinfo"
  },
  "exclude": [
    "node_modules/**",
    "templates/**",
    "dist/**",
    "esm/**",
    ".stan/**",
    "tools/**",
    "**/.rollup.cache/**",
    "**/.tsbuild/**"
  ],
  "extends": "./tsconfig.base.json",
  "include": ["**/*", "**/*.json"]
}
